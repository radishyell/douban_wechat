
.default_font {
	color: #333333;
}
.default_gray_font {
	color: #999999;
}

.empty_content {
	width: 100%;
	height: 100%;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 30rpx;
	text-align: center;
	letter-spacing: 2rpx;
}

.empty_image {
	width: 300rpx;
	height: auto;
}

.empty_middle {
	margin: 30rpx auto;
}

.empty_btn {
	width: 200rpx;
	height: 70rpx;
	line-height: 70rpx;
	text-align: center;
	margin: 0rpx auto;
	border-radius: 10rpx;
	background: #c2dfee;
}